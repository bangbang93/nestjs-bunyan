{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "lib": ["es5", "es6", "es7", "es2018", "es2019", "es2020", "esnext"],
    "inlineSourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "dist",
    "rootDir": "src"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
